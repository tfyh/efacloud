2;api;U2fWGk|new_members;Mitgliedsnummer,Vorname,Nachname,Geschlecht,Geburtsdatum;Mitgliederliste;(ISNULL(`Austritt`) OR (`Austritt` = '') OR (YEAR(`Austritt`) < 1910) OR (DATEDIFF(NOW(), `Austritt`) < 0)) AND (`Rolle` = 'Ruderkurs' OR (DATEDIFF(NOW(), `Eintritt`) > -35)) AND ((`efaGUID` IS NULL) OR (`efaGUID`=''));sort=Mitgliedsnummer
